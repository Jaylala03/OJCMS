@model eCMS.DataLogic.Models.Case
@using eCMS.Shared
@{
    Layout = "~/Views/Shared/_LayoutInternal.cshtml";
    ViewBag.Title = "Create New Case";
}
@section headsection
{
    @Styles.Render("~/assets/plugins/kendo/css")
    @Scripts.Render("~/assets/plugins/kendo/js")
}
<div class="container bulk-import">
    <!-- begin: mainpage -->
    <div class="row margin-top-40">
         <div class="col-md-12">
            @using (Html.BeginForm(Constants.Actions.Create, Constants.Controllers.Case, new { area = Constants.Areas.CaseManagement }, FormMethod.Post, new { id = "frmEditor", role = "form" }))
            {
                @Html.Partial("_CreateOrEdit", Model)
            }
        </div>
    </div>
</div>
