@model eCMS.DataLogic.Models.Case
@using eCMS.Shared
@{
    ViewBag.Title = "Create New Case";
    Layout = "~/Areas/CaseManagement/Views/Shared/_LayoutCase.cshtml";
}
@section headsection
{
    @Styles.Render("~/assets/plugins/kendo/css")
    @Scripts.Render("~/assets/plugins/kendo/js")
}
@if (Model.IsReadmit)
{
    <div class="container bulk-import">
        <!-- begin: mainpage -->
        <div class="row margin-top-40">
            <div class="col-md-12">
                @using (Html.BeginForm(Constants.Actions.Readmit, Constants.Controllers.Case, new { area = Constants.Areas.CaseManagement }, FormMethod.Post, new { id = "frmEditor", role = "form" }))
                {
                    @Html.Partial("_CreateOrEdit", Model)
                }
            </div>
        </div>
    </div>
}
else
{
    <div class="container bulk-import">
        <!-- begin: mainpage -->
        <div class="row margin-top-40">
            <div class="col-md-12">
                @using (Html.BeginForm(Constants.Actions.Edit, Constants.Controllers.Case, new { area = Constants.Areas.CaseManagement }, FormMethod.Post, new { id = "frmEditor", role = "form" }))
                {
                    @Html.Partial("_CreateOrEdit", Model)
                }
            </div>
        </div>
    </div>
}